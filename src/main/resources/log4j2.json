{
  "configuration": {
    "monitorInterval": "1",
    "name": "Default",
    "appenders": {
      "Console": {
        "name":"Console_1",
        "target":"SYSTEM_OUT",
        "PatternLayout": {
          "pattern":"%d{HH:mm:ss} [%t] %-5level %logger{1} - %msg%n"
        }
      },
      "RollingFile": {
        "name":"File",
        "fileName":"logs/music-app.log",
        "filePattern":"logs/archive/-music-app-backup-%d{MM-dd-yy-HH-mm-ss}-%i.log.zip",
        "PatternLayout": {
          "pattern":"%d{yyyy-MM-dd HH:mm:ss} %-5level %logger{4}:%L - %msg%n"
        },
        "Policies": {
          "SizeBasedTriggeringPolicy": {
            "size":"50 KB"
          }
        },
        "DefaultRolloverStrategy": {
          "max":"10"
        }
      }
    },
    "loggers": {
      "root": {
        "LevelRangeFilter": {
          "minLevel": "fatal",
          "maxLevel": "debug",
          "onMatch": "ACCEPT",
          "onMismatch": "DENY"
        },
        "level":"info",
        "appender-ref": [{
          "ref":"Console_1"
        },
        {
          "ref":"File"
        }]
      }
    }
  }
}